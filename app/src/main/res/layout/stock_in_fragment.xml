        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        >
    <data>
        <variable
                name="viewmodel"
                type="com.mawared.mawaredvansale.controller.inventory.stockin.stockinlist.StockInViewModel" />
